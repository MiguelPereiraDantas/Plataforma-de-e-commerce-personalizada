<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using Web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    No exemplo a seguir, a transformação "SetAttributes" alterará o valor de
    "connectionString" para usar "ReleaseSQLServer" somente quando o localizador "Match"
    encontre um atributo "nome" com o valor "MyDB".

    <connectionStrings>
      <add name="MyDB"
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->
  <system.web>
    <!--
      No exemplo a seguir, a transformação "Replace" substituirá toda a seção de
      <customErrors> do arquivo Web.config.
      Observe que, como há apenas uma sessão customErrors no nó
      <system.web>, não é necessário utilizar o atributo "xdt:Locator".

      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
</configuration>
